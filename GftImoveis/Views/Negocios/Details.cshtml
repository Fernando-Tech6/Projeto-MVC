@model GftImoveis.Models.Negocio

@{
    Layout = "_NovoLayout";
}


@{
    ViewData["Title"] = "Details";
}
<head>
    <link rel="stylesheet" href="~/css/modelo.css">
</head>


<h1>Detalhes</h1>

<div>
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Nome)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Nome)
        </dd>
    </dl>
</div>
<div class="detalhes">
    <a asp-action="Edit" asp-route-id="@Model.NegocioId" class="btn btn-outline-primary">Edit</a> 
    <a asp-action="Index" class="btn btn-outline-dark">Voltar</a>
</div>
